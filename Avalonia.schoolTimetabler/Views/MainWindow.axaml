<Window    x:Class="Avalonia.schoolTimetabler.Views.MainWindow"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:views="using:Avalonia.schoolTimetabler.Views"    xmlns:vm="using:Avalonia.schoolTimetabler.ViewModels"    Title="Школьное расписание"    d:DesignHeight="450"    d:DesignWidth="800"    Icon="/Assets/timetable.ico"    WindowState="Maximized"    mc:Ignorable="d">    <Grid>        <Grid.RowDefinitions>            <RowDefinition Height="Auto" />            <RowDefinition Height="*" />        </Grid.RowDefinitions>        <Menu Grid.Row="0">            <MenuItem Header="{Binding HeaderAString}">                <MenuItem                    Name="CreateSchoolProfileBtn"                    Command="{Binding IsVisibleChange}"                    Header="{Binding CreateSchoolProfileString}" />                <MenuItem Name="SaveSchoolProfileBtn" Header="{Binding SaveSchoolProfileString}" />                <MenuItem Name="LoadSavedSchoolProfileBtn" Header="{Binding LoadSavedSchoolProfileString}" />                <MenuItem Name="DeleteSchoolProfileBtn" Header="{Binding DeleteSchoolProfileString}" />            </MenuItem>            <MenuItem Header="{Binding HeaderBString}">                <MenuItem Name="ViewTimetableBtn" Header="{Binding ViewTimetableString}" />                <MenuItem Name="CreateTimetableBtn" Header="{Binding CreateTimetableString}" />                <MenuItem Name="DeleteTimetableBtn" Header="{Binding DeleteTimetableString}" />            </MenuItem>            <MenuItem Name="SchoolInformationBtn" Header="{Binding SchoolInformationString}" />            <MenuItem Name="HelpBtn" Header="{Binding HelpString}" />        </Menu>        <Grid            Name="WelcomeText"            Grid.Row="1"            IsVisible="{Binding IsVisibleGreeting}">            <TextBlock                HorizontalAlignment="Center"                VerticalAlignment="Center"                Text="{Binding GreetingOneString}" />            <TextBlock                Margin="0,40,0,0"                HorizontalAlignment="Center"                VerticalAlignment="Center"                Text="{Binding GreetingTwoString}" />            <TextBlock                Margin="0,80,0,0"                HorizontalAlignment="Center"                VerticalAlignment="Center"                Text="{Binding GreetingThreeString}" />        </Grid>    </Grid>    <Design.DataContext>        <vm:MainWindowViewModel />    </Design.DataContext></Window>