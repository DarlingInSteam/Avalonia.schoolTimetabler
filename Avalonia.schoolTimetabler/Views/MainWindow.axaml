<Window    x:Class="Avalonia.schoolTimetabler.Views.MainWindow"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:vm="using:Avalonia.schoolTimetabler.ViewModels"    Title="School timetable"    d:DesignHeight="450"    d:DesignWidth="800"    Icon="/Assets/avalonia-logo.ico"    WindowState="Maximized"    mc:Ignorable="d">    <Grid>        <Grid.RowDefinitions>            <RowDefinition Height="Auto" />            <RowDefinition Height="*" />        </Grid.RowDefinitions>        <Menu Grid.Row="0">            <MenuItem Header="Файл">                <MenuItem Name="CreateSchoolProfile" Header="Создать профиль школы" />                <MenuItem Name="SaveSchoolProfile" Header="Сохранить профиль школы" />                <MenuItem Name="LoadSavedSchoolProfile" Header="Загрузить профиль школы" />                <MenuItem Name="DeleteSchoolProfile" Header="Удалить профиль школы" />            </MenuItem>            <MenuItem Header="Расписание">                <MenuItem Name="ViewTimetable" Header="Посмотреть расписание" />                <MenuItem Name="CreateTimetable" Header="Создать расписание автоматически" />                <MenuItem Name="DeleteTimetable" Header="Удалить расписание" />            </MenuItem>            <MenuItem Name="SchoolInformation" Header="Информация о школе" />            <MenuItem Name="Help" Header="Помощь" />        </Menu>        <Grid Grid.Row="1">            <TextBlock                HorizontalAlignment="Center"                VerticalAlignment="Center"                TextAlignment="Center">                Вас приветствует приложение Школьное расписание!                Для продолжения работы, пожалуйста, ознакомьтесь с разделом "Помощь".            </TextBlock>        </Grid>    </Grid>    <Design.DataContext>        <vm:MainWindowViewModel />    </Design.DataContext></Window>