<Window    x:Class="Avalonia.schoolTimetabler.Views.MainWindow"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:vm="using:Avalonia.schoolTimetabler.ViewModels"    Title="School timetable"    d:DesignHeight="450"    d:DesignWidth="800"    Icon="/Assets/avalonia-logo.ico"    WindowState="Maximized"    mc:Ignorable="d">    <StackPanel>        <WrapPanel Background="Black" Orientation="Horizontal">            <TextBlock Foreground="White" Text="Панель информации о школе: " />            <TextBlock                Margin="0,0,0,0"                Foreground="White"                Text="Школа 27" />            <TextBlock                Margin="10,0,0,0"                Foreground="White"                Text="Классов 44" />            <TextBlock                Margin="10,0,0,0"                Foreground="White"                Text="Пользователь: Елена Михайловна" />        </WrapPanel>        <WrapPanel Margin="50,50,0,0" Orientation="Vertical">            <Button Name="Create" Content="Посмотреть расписание" />            <Button Name="CreateTimetable" Content="Создать расписание автоматически" />            <Button Name="DeleteTimetable" Content="Удалить расписание" />            <Button Name="CreateSchoolProfile" Content="Создать профиль школы" />            <Button Name="LoadSavedSchoolProfile" Content="Загрузить сохраненный профиль школы" />            <Button Name="DeleteSchoolProfile" Content="Удалить профиль школы" />        </WrapPanel>    </StackPanel>    <Design.DataContext>        <vm:MainWindowViewModel />    </Design.DataContext></Window>