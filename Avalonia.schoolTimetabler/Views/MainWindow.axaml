<Window    x:Class="Avalonia.schoolTimetabler.Views.MainWindow"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:vm="using:Avalonia.schoolTimetabler.ViewModels"    Title="Школьное расписание"    d:DesignHeight="450"    d:DesignWidth="800"    Icon="/Assets/avalonia-logo.ico"    WindowState="Maximized"    mc:Ignorable="d">    <Grid>        <Grid.RowDefinitions>            <RowDefinition Height="Auto" />            <RowDefinition Height="*" />        </Grid.RowDefinitions>        <Menu Grid.Row="0">            <MenuItem Header="Файл">                <MenuItem                    Name="CreateSchoolProfileBtn"                    Click="CreateSchoolProfile_OnClick"                    Header="Создать профиль школы" />                <MenuItem                    Name="SaveSchoolProfile"                    Click="SaveSchoolProfile_OnClick"                    Header="Сохранить профиль школы" />                <MenuItem                    Name="LoadSavedSchoolProfile"                    Click="LoadSavedSchoolProfile_OnClick"                    Header="Загрузить профиль школы" />                <MenuItem                    Name="DeleteSchoolProfile"                    Click="DeleteSchoolProfile_OnClick"                    Header="Удалить профиль школы" />            </MenuItem>            <MenuItem Header="Расписание">                <MenuItem                    Name="ViewTimetable"                    Click="ViewTimetable_OnClick"                    Header="Посмотреть расписание" />                <MenuItem                    Name="CreateTimetable"                    Click="CreateTimetable_OnClick"                    Header="Создать расписание автоматически" />                <MenuItem                    Name="DeleteTimetable"                    Click="DeleteTimetable_OnClick"                    Header="Удалить расписание" />            </MenuItem>            <MenuItem                Name="SchoolInformation"                Click="SchoolInformation_OnClick"                Header="Информация о школе" />            <MenuItem                Name="Help"                Click="Help_OnClick"                Header="Помощь" />        </Menu>        <Grid Name="WelcomeText" Grid.Row="1">            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center">                Вас приветствует приложение: Школьное расписание.            </TextBlock>            <TextBlock                Margin="0,40,0,0"                HorizontalAlignment="Center"                VerticalAlignment="Center">                Для продолжения работы, пожалуйста, ознакомьтесь с разделом "Помощь".            </TextBlock>            <TextBlock                Margin="0,80,0,0"                HorizontalAlignment="Center"                VerticalAlignment="Center">                Если уже ознакомлены, то можете создать профиль школы или загрузить имеющийся в разделе "Файл".            </TextBlock>        </Grid>        <Grid Grid.Row="1" />    </Grid>    <Design.DataContext>        <vm:MainWindowViewModel />    </Design.DataContext></Window>