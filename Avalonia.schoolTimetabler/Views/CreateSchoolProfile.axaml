<UserControl    x:Class="Avalonia.schoolTimetabler.Views.CreateSchoolProfile"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:reactiveUi="http://reactiveui.net"    xmlns:rxui="clr-namespace:Avalonia.ReactiveUI;assembly=Avalonia.ReactiveUI"    xmlns:schoolTimetabler="clr-namespace:Avalonia.schoolTimetabler"    xmlns:v="clr-namespace:Avalonia.schoolTimetabler.Views"    xmlns:vm="clr-namespace:Avalonia.schoolTimetabler.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Grid>        <Grid.RowDefinitions>            <RowDefinition Height="Auto" />            <RowDefinition Height="*" />        </Grid.RowDefinitions>        <Grid.ColumnDefinitions>            <ColumnDefinition Width="*" />            <ColumnDefinition Width="*" />        </Grid.ColumnDefinitions>        <StackPanel            Margin="15,55,0,0"            HorizontalAlignment="Left"            VerticalAlignment="Top">            <TextBlock>Информация о пользователе:</TextBlock>            <WrapPanel Margin="0,15,0,0" Orientation="Horizontal">                <StackPanel Orientation="Vertical" Spacing="5">                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding FullName}"                        TextAlignment="Center"                        Watermark="ФИО редактора" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding Post}"                        TextAlignment="Center"                        Watermark="Должность редактора" />                    <Button VerticalAlignment="Bottom" Command="{Binding ConfirmUserSettings}">Подтвердить</Button>                </StackPanel>            </WrapPanel>        </StackPanel>        <StackPanel            Grid.Column="0"            Margin="15,0,0,0"            HorizontalAlignment="Left"            VerticalAlignment="Center">            <TextBlock>Информация о школе:</TextBlock>            <WrapPanel Margin="0,15,0,0" Orientation="Horizontal">                <StackPanel Orientation="Vertical" Spacing="5">                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding SchoolNumber}"                        TextAlignment="Center"                        Watermark="Номер школы" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding FullNameDirector}"                        TextAlignment="Center"                        Watermark="ФИО директора" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding CountClasses}"                        TextAlignment="Center"                        Watermark="Количество классов" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding CountTeachers}"                        TextAlignment="Center"                        Watermark="Количество уителей" />                    <Button VerticalAlignment="Bottom" Command="{Binding ConfirmSchoolSettings}">Подтвердить</Button>                </StackPanel>                <StackPanel                    Margin="10,0,0,0"                    Orientation="Vertical"                    Spacing="5">                    <Button Content="Меню редактрования дисциплин" />                    <Button Content="Меню редактирования классов" />                </StackPanel>            </WrapPanel>        </StackPanel>        <StackPanel            Grid.Row="0"            Grid.Column="1"            Background="Black">            <rxui:RoutedViewHost Margin="0,55,0,0" Router="{Binding Router}">                <rxui:RoutedViewHost.ViewLocator>                    <schoolTimetabler:AppViewLocator />                </rxui:RoutedViewHost.ViewLocator>            </rxui:RoutedViewHost>        </StackPanel>    </Grid>    <Design.DataContext>        <vm:CreateSchoolProfileViewModel />    </Design.DataContext></UserControl>