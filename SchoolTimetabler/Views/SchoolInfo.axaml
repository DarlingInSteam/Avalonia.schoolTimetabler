<UserControl    x:Class="SchoolTimetabler.Views.SchoolInfo"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:vm="using:SchoolTimetabler.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Grid>        <TabControl Margin="5,0,5,0" TabStripPlacement="Left">            <TabItem>                <TabItem.Header>                    <TextBlock Text="Информация о пользователе" />                </TabItem.Header>                <Border                    Margin="20"                    Padding="4"                    HorizontalAlignment="Center"                    VerticalAlignment="Top"                    BorderBrush="White"                    BorderThickness="2"                    CornerRadius="10">                    <StackPanel Orientation="Vertical" Spacing="10">                        <StackPanel Orientation="Vertical" Spacing="10">                            <Label HorizontalAlignment="Center">ФИО редактора</Label>                            <TextBlock HorizontalAlignment="Center" Text="{Binding FullName}" />                        </StackPanel>                        <StackPanel Orientation="Vertical" Spacing="10">                            <Label HorizontalAlignment="Center">Должность Редактора</Label>                            <TextBlock HorizontalAlignment="Center" Text="{Binding Post}" />                        </StackPanel>                    </StackPanel>                </Border>            </TabItem>            <TabItem>                <TabItem.Header>                    <TextBlock Text="Информация о школе" />                </TabItem.Header>                <Border                    Margin="20"                    Padding="4"                    HorizontalAlignment="Center"                    VerticalAlignment="Top"                    BorderBrush="White"                    BorderThickness="2"                    CornerRadius="10">                    <StackPanel Orientation="Horizontal" Spacing="10">                        <StackPanel Orientation="Vertical" Spacing="10">                            <Label HorizontalAlignment="Center">Номер школы</Label>                            <TextBlock HorizontalAlignment="Center" Text="{Binding SchoolNumber}" />                        </StackPanel>                        <StackPanel Orientation="Vertical" Spacing="10">                            <Label HorizontalAlignment="Center">ФИО директора</Label>                            <TextBlock HorizontalAlignment="Center" Text="{Binding FullNameDirector}" />                        </StackPanel>                        <StackPanel Orientation="Vertical" Spacing="10">                            <Label HorizontalAlignment="Center">Количество классов в школе</Label>                            <TextBlock HorizontalAlignment="Center" Text="{Binding CountClasses}" />                        </StackPanel>                        <StackPanel Orientation="Vertical" Spacing="10">                            <Label HorizontalAlignment="Center">Количество учителей в школе</Label>                            <TextBlock HorizontalAlignment="Center" Text="{Binding CountTeacher}" />                        </StackPanel>                    </StackPanel>                </Border>            </TabItem>            <TabItem>                <TabItem.Header>                    <TextBlock>Классы</TextBlock>                </TabItem.Header>                <DataGrid                    MaxHeight="400"                    Margin="20"                    VerticalAlignment="Top"                    AutoGenerateColumns="True"                    IsReadOnly="True"                    Items="{Binding Classes}" />            </TabItem>            <TabItem>                <TabItem.Header>                    <TextBlock>Дисциплины</TextBlock>                </TabItem.Header>                <DataGrid                    MaxHeight="400"                    Margin="20"                    VerticalAlignment="Top"                    AutoGenerateColumns="True"                    IsReadOnly="True"                    Items="{Binding Disciplines}" />            </TabItem>            <TabItem>                <TabItem.Header>                    <TextBlock>Учителя</TextBlock>                </TabItem.Header>                <StackPanel                    Margin="20"                    HorizontalAlignment="Stretch"                    VerticalAlignment="Top"                    Spacing="20">                    <ComboBox                        Width="200"                        Height="50"                        HorizontalAlignment="Center"                        Items="{Binding TeachersName}"                        SelectedIndex="{Binding SelectedIndexComboBoxTeachers}" />                    <ListBox MaxHeight="400" Items="{Binding DisciplinesName}" />                </StackPanel>            </TabItem>            <TabItem>                <TabItem.Header>                    <TextBlock>Кабинеты</TextBlock>                </TabItem.Header>                <DataGrid                    MaxHeight="400"                    Margin="20"                    VerticalAlignment="Top"                    AutoGenerateColumns="True"                    IsReadOnly="True"                    Items="{Binding Cabinets}" />            </TabItem>        </TabControl>    </Grid>    <Design.DataContext>        <vm:SchoolInfoViewModel />    </Design.DataContext></UserControl>