<UserControl    x:Class="SchoolTimetabler.Views.UserInformation"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:viewModels1="clr-namespace:SchoolTimetabler.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Design.DataContext>        <viewModels1:UserInformationViewModel />    </Design.DataContext>    <Border        Padding="4"        BorderBrush="White"        BorderThickness="2"        CornerRadius="10">        <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top">            <TextBlock HorizontalAlignment="Center">Информация о пользователе:</TextBlock>            <WrapPanel Margin="0,15,0,0" Orientation="Horizontal">                <StackPanel Orientation="Vertical" Spacing="5">                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding FullName}"                        TextAlignment="Center"                        Watermark="ФИО редактора" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding Post}"                        TextAlignment="Center"                        Watermark="Должность редактора" />                    <Button                        Margin="0,10,0,0"                        HorizontalAlignment="Center"                        VerticalAlignment="Bottom"                        Command="{Binding ConfirmUserSettings}">                        Подтвердить                    </Button>                </StackPanel>            </WrapPanel>        </StackPanel>    </Border></UserControl>