<Window    x:Class="SchoolTimetabler.Views.PasswordWindow"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:local="using:SchoolTimetabler.Views"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    Title="Установить пароль"    d:DesignHeight="450"    d:DesignWidth="800"    SizeToContent="Width"    mc:Ignorable="d">    <Panel>        <ExperimentalAcrylicBorder IsHitTestVisible="False">            <ExperimentalAcrylicBorder.Material>                <ExperimentalAcrylicMaterial                    BackgroundSource="Digger"                    MaterialOpacity="0.65"                    TintColor="Black"                    TintOpacity="1" />            </ExperimentalAcrylicBorder.Material>        </ExperimentalAcrylicBorder>        <Panel>            <local:SetPassword />        </Panel>    </Panel></Window>