<UserControl xmlns="https://github.com/avaloniaui"             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"             xmlns:vm="clr-namespace:SchoolTimetabler.ViewModels"             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"             x:Class="SchoolTimetabler.Views.CreateTimetable">        <Design.DataContext>        <vm:CreateTimetableViewModel />    </Design.DataContext>    <Grid>        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical" Spacing="20">            <TextBlock Text="{Binding DayOfTheWeek, Mode=TwoWay}" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>            <StackPanel Spacing="20" Orientation="Horizontal">                    <TextBlock Width="35" VerticalAlignment="Center">1-ый</TextBlock>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexTeacherMon}" Items="{Binding TeachersName}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexDisciplineMon}" Items="{Binding DisciplinesTeacherMon}"> </ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexCabinetMon}" Items="{Binding CabinetsNumbers}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexClassMon}" Items="{Binding ClassesNumber}"></ComboBox>                </StackPanel>                <StackPanel Spacing="20" Orientation="Horizontal">                    <TextBlock Width="35" VerticalAlignment="Center">2-ой</TextBlock>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexTeacherTues}" Items="{Binding TeachersName}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexDisciplineTues}" Items="{Binding DisciplinesTeacherTues}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexCabinetTues}" Items="{Binding CabinetsNumbers}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexClassTues}" Items="{Binding ClassesNumber}"></ComboBox>                </StackPanel>                <StackPanel Spacing="20" Orientation="Horizontal">                    <TextBlock Width="35" VerticalAlignment="Center">3-ий</TextBlock>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexTeacherWed}" Items="{Binding TeachersName}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexDisciplineWed}" Items="{Binding DisciplinesTeacherWed}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexCabinetWed}" Items="{Binding CabinetsNumbers}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexClassWed}" Items="{Binding ClassesNumber}"></ComboBox>                </StackPanel>                <StackPanel Spacing="20" Orientation="Horizontal">                    <TextBlock Width="35" VerticalAlignment="Center">4-ый</TextBlock>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexTeacherThurs}" Items="{Binding TeachersName}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexDisciplineThurs}" Items="{Binding DisciplinesTeacherThurs}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexCabinetThurs}" Items="{Binding CabinetsNumbers}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexClassThurs}" Items="{Binding ClassesNumber}"></ComboBox>                </StackPanel>                <StackPanel Spacing="20" Orientation="Horizontal">                    <TextBlock Width="35" VerticalAlignment="Center">5-ый</TextBlock>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexTeacherFri}" Items="{Binding TeachersName}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexDisciplineFri}" Items="{Binding DisciplinesTeacherFri}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexCabinetFri}" Items="{Binding CabinetsNumbers}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexClassFri}" Items="{Binding ClassesNumber}"></ComboBox>                </StackPanel>                <StackPanel Spacing="20" Orientation="Horizontal">                    <TextBlock Width="35" VerticalAlignment="Center">6-ой</TextBlock>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexTeacherSat}" Items="{Binding TeachersName}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexDisciplineSat}" Items="{Binding DisciplinesTeacherSat}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexCabinetSat}" Items="{Binding CabinetsNumbers}"></ComboBox>                    <ComboBox Width="150" MaxWidth="150" SelectedIndex="{Binding SelectedIndexClassSat}" Items="{Binding ClassesNumber}"></ComboBox>                </StackPanel>        </StackPanel>                <Button Command="{Binding BackDay}" IsEnabled="{Binding IsEnableBack}" Background="Black" Margin="550,0,0,0" Width="50" Height="50" HorizontalAlignment="Left" VerticalAlignment="Center">            <Image Source="/Assets/leftIcon.png"/>        </Button>                <Button Command="{Binding NextDay}" IsEnabled="{Binding IsEnableNext}" Background="Black" Margin="0,0,550,0" Width="50" Height="50" HorizontalAlignment="Right" VerticalAlignment="Center">            <Image Source="/Assets/rightIcon.png"/>        </Button>                                        <Button Command="{Binding SaveOneTimetable}" Width="50" Height="50" Background="Black" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,350,550,0">            <Image Source="/Assets/done.png"></Image>        </Button>    </Grid>    </UserControl>