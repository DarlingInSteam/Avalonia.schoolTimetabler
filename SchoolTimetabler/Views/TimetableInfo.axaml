<UserControl    x:Class="SchoolTimetabler.Views.TimetableInfo"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:viewModels="clr-namespace:SchoolTimetabler.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Grid>        <StackPanel            HorizontalAlignment="Center"            VerticalAlignment="Center"            Orientation="Vertical"            Spacing="20">            <StackPanel                HorizontalAlignment="Center"                VerticalAlignment="Center"                Orientation="Horizontal"                Spacing="20">                <TextBlock                    Width="150"                    MaxWidth="150"                    Text="{Binding DayOfTheWeek, Mode=TwoWay}" />                <ComboBox Items="{Binding ClassesNumber}" SelectedIndex="{Binding SelectedIndexComboBox}" />            </StackPanel>            <StackPanel Orientation="Horizontal">                <ListBox                    Width="150"                    MaxWidth="150"                    Items="{Binding LessonNumbers}" />                <ListBox                    Width="150"                    MaxWidth="150"                    Items="{Binding TeacherName}" />                <ListBox                    Width="150"                    MaxWidth="150"                    Items="{Binding DisciplineName}" />                <ListBox                    Width="150"                    MaxWidth="150"                    Items="{Binding CabinetNumber}" />                <ListBox                    Width="150"                    MaxWidth="150"                    Items="{Binding ClassesNumbers}" />            </StackPanel>            <Button Command="{Binding DelTimetable}" Content="Удалить данное расписание" />        </StackPanel>        <Button            Width="50"            Height="50"            Margin="450,0,0,0"            HorizontalAlignment="Left"            VerticalAlignment="Center"            Command="{Binding BackDay}"            IsEnabled="{Binding IsEnableBack}">            <Image Source="/Assets/leftIcon.png" />        </Button>        <Button            Width="50"            Height="50"            Margin="40,0,450,0"            HorizontalAlignment="Right"            VerticalAlignment="Center"            Command="{Binding NextDay}"            IsEnabled="{Binding IsEnableNext}">            <Image Source="/Assets/rightIcon.png" />        </Button>    </Grid>    <Design.DataContext>        <viewModels:TimetableInfoViewModel />    </Design.DataContext></UserControl>