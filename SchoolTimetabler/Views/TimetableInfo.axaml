<UserControl xmlns="https://github.com/avaloniaui"             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"             xmlns:viewModels="clr-namespace:SchoolTimetabler.ViewModels"             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"             x:Class="SchoolTimetabler.Views.TimetableInfo">        <Grid>        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical" Spacing="20">            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="20">                <TextBlock Width="150" MaxWidth="150" Text="{Binding DayOfTheWeek, Mode=TwoWay}"></TextBlock>                <ComboBox SelectedIndex="{Binding SelectedIndexComboBox}" Items="{Binding ClassesNumber}"></ComboBox>            </StackPanel>            <StackPanel Spacing="20" Orientation="Horizontal">                <ListBox Items="{Binding TeacherName}" Width="150" MaxWidth="150"></ListBox>                <ListBox Items="{Binding DisciplineName}" Width="150" MaxWidth="150"></ListBox>                <ListBox Items="{Binding CabinetNumber}" Width="150" MaxWidth="150"></ListBox>                <ListBox Items="{Binding ClassesNumbers}" Width="150" MaxWidth="150"></ListBox>            </StackPanel>        </StackPanel>                <Button Command="{Binding BackDay}" IsEnabled="{Binding IsEnableBack}" Background="Black" Margin="550,0,0,0" Width="50" Height="50" HorizontalAlignment="Left" VerticalAlignment="Center">            <Image Source="/Assets/leftIcon.png"/>        </Button>                <Button Command="{Binding NextDay}" IsEnabled="{Binding IsEnableNext}" Background="Black" Margin="0,0,550,0" Width="50" Height="50" HorizontalAlignment="Right" VerticalAlignment="Center">            <Image Source="/Assets/rightIcon.png"/>        </Button>    </Grid>        <Design.DataContext>        <viewModels:TimetableInfoViewModel />    </Design.DataContext></UserControl>