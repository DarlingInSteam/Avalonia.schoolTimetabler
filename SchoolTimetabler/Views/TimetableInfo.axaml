<UserControl    x:Class="SchoolTimetabler.Views.TimetableInfo"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:viewModels="clr-namespace:SchoolTimetabler.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Grid>        <StackPanel            HorizontalAlignment="Center"            VerticalAlignment="Center"            Orientation="Vertical"            Spacing="20">            <StackPanel                HorizontalAlignment="Center"                VerticalAlignment="Center"                Orientation="Horizontal"                Spacing="20">                <TextBlock                    Width="150"                    MaxWidth="150"                    Text="{Binding DayOfTheWeek, Mode=TwoWay}" />                <ComboBox Items="{Binding ClassesNumber}" SelectedIndex="{Binding SelectedIndexComboBox}" />            </StackPanel>            <Border                Margin="0,10,0,0"                Padding="4"                BorderBrush="White"                BorderThickness="2"                CornerRadius="3"                IsVisible="{Binding IsVisibleBorder}">                <StackPanel Orientation="Horizontal">                    <ListBox                        Width="150"                        MaxWidth="150"                        Items="{Binding TeacherName}" />                    <ListBox                        Width="150"                        MaxWidth="150"                        Items="{Binding DisciplineName}" />                    <ListBox                        Width="150"                        MaxWidth="150"                        Items="{Binding CabinetNumber}" />                    <ListBox                        Width="150"                        MaxWidth="150"                        Items="{Binding ClassesNumbers}" />                </StackPanel>            </Border>        </StackPanel>        <Button            Width="50"            Height="50"            Margin="550,0,0,0"            HorizontalAlignment="Left"            VerticalAlignment="Center"            Background="Black"            Command="{Binding BackDay}"            IsEnabled="{Binding IsEnableBack}">            <Image Source="/Assets/leftIcon.png" />        </Button>        <Button            Width="50"            Height="50"            Margin="0,0,550,0"            HorizontalAlignment="Right"            VerticalAlignment="Center"            Background="Black"            Command="{Binding NextDay}"            IsEnabled="{Binding IsEnableNext}">            <Image Source="/Assets/rightIcon.png" />        </Button>    </Grid>    <Design.DataContext>        <viewModels:TimetableInfoViewModel />    </Design.DataContext></UserControl>