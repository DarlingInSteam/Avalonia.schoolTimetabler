<UserControl    x:Class="SchoolTimetabler.Views.SchoolInformation"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:viewModels="clr-namespace:SchoolTimetabler.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Design.DataContext>        <viewModels:SchoolInformationViewModel />    </Design.DataContext>    <Border        Padding="4"        BorderBrush="White"        BorderThickness="2"        CornerRadius="10">        <StackPanel>            <TextBlock HorizontalAlignment="Center">Информация о школе:</TextBlock>            <WrapPanel Margin="0,15,0,0" Orientation="Horizontal">                <StackPanel Orientation="Vertical" Spacing="5">                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding SchoolNumber}"                        TextAlignment="Center"                        Watermark="Номер школы" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding FullNameDirector}"                        TextAlignment="Center"                        Watermark="ФИО директора" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding CountClasses}"                        TextAlignment="Center"                        Watermark="Количество классов" />                    <TextBox                        Width="235"                        Height="40"                        Text="{Binding CountTeachers}"                        TextAlignment="Center"                        Watermark="Количество уителей" />                    <Button Margin="0,10,0,0" Command="{Binding ConfirmSchoolSettings}">                        Подтвердить                    </Button>                </StackPanel>            </WrapPanel>        </StackPanel>    </Border></UserControl>