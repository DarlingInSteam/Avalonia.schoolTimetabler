<Application    x:Class="SchoolTimetabler.App"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles">    <Application.Styles>        <Style Selector="TextBlock">            <Setter Property="FontFamily" Value="Roboto" />        </Style>        <Style Selector="DataGrid">            <Setter Property="FontFamily" Value="Roboto" />        </Style>        <Style Selector="TextBox">            <Setter Property="FontFamily" Value="Roboto" />        </Style>        <Style Selector="ComboBox">            <Setter Property="FontFamily" Value="Roboto" />        </Style>        <Style Selector="Label">            <Setter Property="FontFamily" Value="Roboto" />        </Style>        <Style Selector="TextBox">            <Setter Property="BorderBrush" Value="White" />        </Style>        <FluentTheme Mode="Dark" />        <StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml" />    </Application.Styles></Application>